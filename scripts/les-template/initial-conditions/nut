FoamFile
{
  format ascii;
  class volScalarField;
  object nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 2 -1 0 0 0 0];

internalField uniform 0;

boundaryField
{
  inlets
  {
    type zeroGradient;
  }

  outlets
  {
    type zeroGradient;
  }

  walls
  {
    type zeroGradient;
  }
}

